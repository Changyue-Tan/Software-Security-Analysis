target datalayout = "e-m:o-i64:64-i128:128-n32:64-S128"
target triple = "arm64-apple-macosx14.0.0"

@global_obj_f = global i32 0, align 4
@global_ptr_f = global ptr @global_obj_f, align 8

; Function Attrs: noinline nounwind ssp uwtable(sync)
define i32 @main() #0 {
entry:
  %a = alloca i32, align 4
  store i32 5, ptr %a, align 4
  %0 = load ptr, ptr @global_ptr_f, align 8
  call void @NOALIAS(ptr noundef %0, ptr noundef %a)
  ret i32 0
}

declare void @NOALIAS(ptr noundef, ptr noundef) #1

