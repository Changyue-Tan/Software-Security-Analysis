; ModuleID = 'CI-global.ll'
source_filename = "CI-global.c"
target datalayout = "e-m:o-i64:64-i128:128-n32:64-S128"
target triple = "arm64-apple-macosx14.0.0"

@global = global i32 0, align 4
@p_global = global ptr null, align 8

; Function Attrs: noinline nounwind ssp uwtable(sync)
define void @foo() #0 {
entry:
  store ptr @global, ptr @p_global, align 8
  ret void
}

; Function Attrs: noinline nounwind ssp uwtable(sync)
define i32 @main() #0 {
entry:
  call void @foo()
  %0 = load ptr, ptr @p_global, align 8
  call void @MAYALIAS(ptr noundef @global, ptr noundef %0)
  ret i32 0
}

declare void @MAYALIAS(ptr noundef, ptr noundef) #1

attributes #0 = { noinline nounwind ssp uwtable(sync) "frame-pointer"="non-leaf" "no-trapping-math"="true" "stack-protector-buffer-size"="8" "target-cpu"="apple-m1" "target-features"="+aes,+crc,+crypto,+dotprod,+fp-armv8,+fp16fml,+fullfp16,+lse,+neon,+ras,+rcpc,+rdm,+sha2,+sha3,+sm4,+v8.1a,+v8.2a,+v8.3a,+v8.4a,+v8.5a,+v8a,+zcm,+zcz" }
